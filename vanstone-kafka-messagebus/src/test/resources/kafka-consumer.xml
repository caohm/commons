<?xml version="1.0" encoding="utf-8"?>
<kafka-consumer-group>
	<!-- zookeeper配置项 -->
	<zookeeper.connect></zookeeper.connect>
	<zookeeper.session.timeout.ms>400</zookeeper.session.timeout.ms>
	<zookeeper.sync.time.ms>200</zookeeper.sync.time.ms>
	<auto.commit.interval.ms>1000</auto.commit.interval.ms>
	<consumer id="c1">
		<!-- groupid -->
		<groupid></groupid>
		<!-- 消息主题 -->
		<topic></topic>
		<!-- 消息描述 -->
		<topic-desc></topic-desc>
		<!-- 线程池数量 -->
		<executeservice-count></executeservice-count>
		<!-- 消费者class -->
		<consumerclass></consumerclass>
	</consumer>
	<consumer id="c2">
		<!-- groupid -->
		<groupid></groupid>
		<!-- 消息主题 -->
		<topic></topic>
		<!-- 消息描述 -->
		<topic-desc></topic-desc>
		<!-- 线程池数量 -->
		<executeservice-count></executeservice-count>
		<!-- 消费者class -->
		<consumerclass></consumerclass>
	</consumer>
</kafka-consumer-group>